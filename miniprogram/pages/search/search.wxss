.page {
  width: 100%;
  height: 100%;
}
.page .search-part .weui-search-bar__input,
.page .search-part .weui-search-bar__text {
  height: 26px;
  line-height: 26px;
  font-size: 28rpx;
}
.page .search-part .weui-icon-search {
  font-size: 14px;
}
.page .search-part .weui-icon-search_in-box {
  margin-top: -6px;
}
.page .search-part .weui-search-bar__cancel-btn {
  height: 26rpx;
  line-height: 26rpx;
  font-size: 28rpx;
}
.page .history-part,
.page .host-part {
  padding: 20rpx;
}
.page .history-part .title,
.page .host-part .title {
  font-size: 32rpx;
  height: 60rpx;
  line-height: 60rpx;
  color: #999999;
}
.page .history-part .btn-list-box button,
.page .host-part .btn-list-box button {
  float: left;
  height: 55rpx;
  line-height: 55rpx;
  margin: 10rpx 20rpx;
  border-radius: 30rpx;
  color: #666666;
  border: 1px solid #999999;
  font-size: 0;
  padding: 0 20rpx;
}
.page .history-part .btn-list-box button text,
.page .host-part .btn-list-box button text {
  font-size: 24rpx;
  height: 53rpx;
  line-height: 53rpx;
  display: inline-block;
  vertical-align: middle;
}
.page .history-part .btn-list-box button .delete-icon,
.page .host-part .btn-list-box button .delete-icon {
  height: 20px;
  line-height: 20px;
  margin-left: 10rpx;
}
.page .history-part .clear-box,
.page .host-part .clear-box {
  font-size: 0;
  text-align: center;
  margin-top: 20rpx;
}
.page .history-part .clear-box button,
.page .host-part .clear-box button {
  height: 55rpx;
  line-height: 55rpx;
  font-size: 24rpx;
  color: #ffffff;
}
.page .history-part .clear-box .delete,
.page .host-part .clear-box .delete {
  background: #E64340;
}
.page .history-part .clear-box .finish,
.page .host-part .clear-box .finish {
  background: #1AAD19;
}
.page .matching-part .type-part {
  width: 100%;
  background: #EDEDED;
}
.page .matching-part .type-part .type-item {
  height: 24px;
  line-height: 24px;
  text-align: center;
  font-size: 24rpx;
  font-weight: bold;
  color: #333333;
  float: left;
  width: 14%;
  margin: 0 3%;
  border-bottom: 2px solid #EDEDED;
}
.page .matching-part .type-part .active {
  border-bottom: 2px solid #E64340;
  color: #E64340;
}
.page .matching-part .matching-item {
  padding: 20rpx 30rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 28rpx;
  color: #666666;
  border-bottom: 1px solid #eeeeee;
  display: flex;
}
.page .matching-part .matching-item .img-box {
  width: 80rpx;
  height: 80rpx;
  border-radius: 4rpx;
  overflow: hidden;
  margin-right: 30rpx;
}
.page .matching-part .matching-item .img-box image {
  width: 80rpx;
  height: 80rpx;
}
.page .matching-part .nomore-item {
  height: 80rpx;
  line-height: 80rpx;
  font-size: 24rpx;
  color: #666666;
  text-align: center;
}
